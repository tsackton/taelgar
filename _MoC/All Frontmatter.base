views:
  - type: table
    name: Table
    filters:
      and:
        - "!typeOf.isEmpty()"
    order:
      - file.name
      - tags
      - typeOf
      - typeOfAlias
      - subtypeOf
    sort:
      - property: tags
        direction: ASC
      - property: typeOfAlias
        direction: ASC
      - property: subtypeOf
        direction: ASC
    columnSize:
      note.tags: 374
